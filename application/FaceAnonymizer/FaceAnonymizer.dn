//uses data.FaceMetadata

component provides FaceAnonymizer.FaceAnonymizer requires FaceAnonymizer.FaceDetector, FaceAnonymizer.FaceModificator, io.Output out {
  FaceDetector fd = new FaceDetector()
  FaceModificator fm = new FaceModificator()
  Image FaceAnonymizer:anonymizeFaces(Image image, ImageMetadata imageMetadata) {


    FaceMetadata facesMetadata[] = fd.detectFaces(image, imageMetadata)
    Image modifiedImage = fm.modifyFaces(image, facesMetadata)
    return modifiedImage
  }
}
